---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Listado de lectura de Marina Tetzlaff</title>
	<link href="http://matetzlaff.github.io/libros/atom.xml" rel="self"/>
	<link href="http://matetzlaff.github.io/libros"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://matetzlaff.github.io/libros</id>
	<author>
		<name>Marina Tetzlaff</name>
		<email>mtetzlaff.econ@gmail.com</email>
	</author>

	{% for libro in site.libros %}
		<entry>
			<title>{{ libro.title }}</title>
			<link href="http://matetzlaff.github.io{{ libro.url }}"/>
			<updated>{{ libro.date | date_to_xmlschema }}</updated>
			<id>http://matetzlaff.github.io{{ libro.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
